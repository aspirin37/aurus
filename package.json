{
  "name": "front-aurus",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8082",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "check-audit": "npx check-audit",
    "setup-lint": "npm i --g eslint babel-eslint eslint-plugin-import eslint-config-airbnb-base eslint-plugin-vue eslint-plugin-jest",
    "test": "jest --config test/jest.conf.js --verbose",
    "clint": "eslint --format junit --ext .js,.vue src",
    "ctest": "jest --ci --config test/jest-ct.conf.js"
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "node-sass": "^4.12.0",
    "npm-audit-resolver": "^2.2.0",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.3",
    "webpack": "^4.43.0",
    "copy-webpack-plugin": "latest"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "jest": true
    },
    "globals": {
      "process": "readonly"
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:vue/strongly-recommended",
      "plugin:vue/recommended",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/max-attributes-per-line": 1,
      "linebreak-style": "off"
    },
    "plugins": [
      "vue"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
