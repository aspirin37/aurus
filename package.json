{
  "name": "front-aurus",
  "version": "0.1.18",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8082",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "check-audit": "npx check-audit",
    "clint": "eslint --format junit --ext .js,.vue src",
    "ctest": "jest --ci --config test/jest-ct.conf.js",
    "setup-lint": "npm i --g eslint babel-eslint eslint-plugin-import eslint-config-airbnb-base eslint-plugin-vue eslint-plugin-jest",
    "test": "jest --config test/jest.conf.js --verbose"
  },
  "dependencies": {
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.14.0",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "html-webpack-plugin": "^4.3.0",
    "material-design-icons-iconfont": "^5.0.1",
    "moment-timezone": "^0.5.31",
    "uuid": "^8.1.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.7",
    "vue-moment": "^4.1.0",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.1.6",
    "vue-select": "^3.10.5",
    "vuetify": "^2.2.28",
    "vuex": "^3.1.3",
    "webpack": "^4.43.0",
    "yaml-loader": "^0.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.4.1",
    "copy-webpack-plugin": "latest",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "node-sass": "^4.12.0",
    "npm-audit-resolver": "^2.2.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-jest": "^3.0.5",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0",
    "vuex": "^3.1.3",
    "webpack": "^4.43.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "jest": true
    },
    "globals": {
      "process": "readonly"
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:vue/strongly-recommended",
      "plugin:vue/recommended",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/max-attributes-per-line": 1,
      "vue/no-v-html": "off",
      "linebreak-style": "off",
      "import/no-cycle": "off",
      "no-param-reassign": "off"
    },
    "plugins": [
      "vue"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
